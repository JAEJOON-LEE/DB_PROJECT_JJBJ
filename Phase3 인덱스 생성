CREATE INDEX INDEX_CATEGORY ON CATEGORY (BIGTYPE, SMALLTYPE);
CREATE INDEX INDEX_PROFIT ON PROFITS (P_ID);
CREATE INDEX INDEX_CUSTOMER ON CUSTOMER (ID);
CREATE INDEX INDEX_ITEM ON ITEMS (PID);
CREATE INDEX INDEX_ORIGIN ON ORIGIN (M_NAME);
CREATE INDEX INDEX_RETAILER ON RETAILER (R_NUM);
CREATE INDEX INDEX_SHOPPING ON SHOPPING (C_ID);
CREATE INDEX INDEX_ORDERINFO ON ORDER_DETAIL (C_ID);

